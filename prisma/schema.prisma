generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model shortenedUrl {
  id        String   @id @default(cuid())
  targetUrl String
  alias     String   @unique
  clicks    Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
